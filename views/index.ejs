    <div class="album py-5 bg-light" style="padding-left: 265px" >
      <div class="container">
  
        <div class="row">

            
            <% for(var i=0;i< dir.length;i++){ %>
              
              <div class="col-md-2">
                  <div class="card mb-2 shadow-sm">
                      <a href="?path=<%= path %>/<%= dir[i]%>"><svg class="bd-placeholder-img card-img-top" width="100%" height="125" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: Thumbnail"><rect width="100%" height="100%" fill="#55595c"/><image xlink:href="/images/dir.png" height="120" width="145"/></svg></a>
                    <div class="card-body">
                        <a href="?path=<%= path %>/<%= dir[i]%>"><%= dir[i]%></a>
                    </div>
                  </div>
                </div>
              <%  } %>

              <% for(var i=0;i< file.length;i++){ %>
          <div class="col-md-2">
            <div class="card mb-2 shadow-sm">
                <a href="/file?path=<%= path %>/<%= file[i]%>"><svg class="bd-placeholder-img card-img-top" width="100%" height="125" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: Thumbnail"><rect width="100%" height="100%" fill="#55595c"/><image xlink:href="/images/<%= extn[i]%>.png" height="120" width="145"/></svg></a>
              <div class="card-body">
                  <a href="/file?path=<%= path %>/<%= file[i]%>"><%= file[i] %></a>
